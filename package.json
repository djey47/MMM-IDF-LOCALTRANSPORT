{
  "name": "MMM-IDF-LOCALTRANSPORT",
  "version": "1.0.0",
  "description": "Local transport in IDF based on API module for MagicMirror",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/djey47/MMM-IDF-LOCALTRANSPORT"
  },
  "keywords": [
    "magic mirror",
    "smart mirror",
    "localtransport",
    "module",
    "paris",
    "STIF",
    "RATP",
    "transilien",
    "SNCF",
    "citymapper.com"
  ],
  "author": "djey47",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/djey47/MMM-IDF-LOCALTRANSPORT/issues"
  },
  "homepage": "https://github.com/djey47/MMM-IDF-LOCALTRANSPORT#readme",
  "scripts": {
    "install": "npm run build-prod",
    "build:module": "webpack --mode=development --config tools/webpack/webpack.config.module.js",
    "build:module-prod": "webpack --mode=production --config tools/webpack/webpack.config.module.js",
    "build:helper": "webpack --mode=development --config tools/webpack/webpack.config.helper.js",
    "build:helper-prod": "webpack --mode=production --config tools/webpack/webpack.config.helper.js",
    "build": "npm run build:module && npm run build:helper",
    "build-prod": "npm run build:module-prod && npm run build:helper-prod",
    "analyze:module": "npm run build:module && webpack-bundle-analyzer stats.json .",
    "analyze:helper": "npm run build:helper && webpack-bundle-analyzer stats.json .",
    "lint": "npm run lint:scripts",
    "lint:scripts": "eslint src tools/dev-server && flow check",
    "flow": "flow check",
    "flow:typings": "flow-typed update",
    "test": "jest --config ./tools/test/jest.default.json",
    "jest:watch:module": "jest --config ./tools/test/jest.client.json --watch",
    "jest:watch:helper:": "jest --config ./tools/test/jest.server.json --watch",
    "jest:watch": "jest --config ./tools/test/jest.default.json --watch",
    "precommit": "npm run lint",
    "prepush": "npm test",
    "dev-server": "nodemon --watch ./tools/dev-server ./tools/dev-server/dev-server.js"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "classnames": "^2.2.6",
    "html-beautify": "^1.0.4",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.26",
    "node-forge": "latest",
    "xml2js": "^0.4.21"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-flow": "^7.0.0",
    "app-root-dir": "^1.0.2",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "config": "^3.2.2",
    "dyson": "^2.0.5",
    "eslint": "^6.3.0",
    "eslint-plugin-flowtype": "^4.2.0",
    "flow-bin": "^0.106.3",
    "flow-typed": "^2.6.1",
    "husky": "^3.0.5",
    "jest": "^24.9.0",
    "jest-cli": "^24.9.0",
    "nodemon": "^1.19.1",
    "webpack": "^4.39.3",
    "webpack-bundle-analyzer": "^3.4.1",
    "webpack-cli": "^3.3.7"
  }
}
